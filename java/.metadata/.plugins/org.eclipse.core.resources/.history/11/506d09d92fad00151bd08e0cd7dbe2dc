package sort;

import java.util.Arrays;

import org.junit.Test;

public class InsertSort {

	public void insertSort(int[] arr) {
		int len = arr.length;
		for (int i = 1; i < len; i++) {
			int j;
			for (j = i - 1; j >= 0; j--) {
				if (arr[j] < arr[i]) {
					break;
				}
			}
			if (j != i - 1) {
				int temp = arr[i];
				int k;
				for (k = i - 1; k > j; k--) {
					arr[k + 1] = arr[k];
				}
				arr[k + 1] = temp;
			}
		}
	}

	@Test
	public void testInsertSort() {
		int[] arr = new int[] { 2, 4, 3, 1 };
		insertSort(arr);
		System.out.println(Arrays.toString(arr));
	}
	
	@Test
	public void test() {
		int i = 0;
		for(;i <= 5; i++) {
			if(i == 3) break;
		}
		System.out.println(i);
	}
}
